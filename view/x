#ifndef CLOUD_VIEW_H
#define CLOUD_VIEW_H

#include "../model/obstaclesModel.h" //заментмть потом на <

#include <SDL2/SDL.h>
#include <SDL2/SDL_image.h>


struct CactiView {
    const struct CactiModel* cloud;
    SDL_Texture* textur;
    SDL_Renderer* render; 
};

struct CactiView initCactiView(const struct CactiModel* tmpCacti, SDL_Renderer* tmpRender);
void drawCactiView(struct CactiView* mthis);
void freeCactiView(struct CactiView* mthis);

#endif
